      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:5266"
    },
    "https": {
      "commandName": "Project",
      "launchBrowser": true,
      "launchUrl": "swagger",
      "environmentVariables": {
        "ASPNETCORE_ENVIRONMENT": "Development"
      },
      "dotnetRunMessages": true,
      "applicationUrl": "http://localhost:7079;http://localhost:5266"

    },
